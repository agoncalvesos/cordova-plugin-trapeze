{
  "name": "TemplatePlugin",
  "version": "0.0.1",
  "description": "Template of a cordova plugin with some OutSystems driven examples",
  "scripts": {
    "init-repo": "test -d .git && test -d .githooks && git config core.hooksPath .githooks",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "cordova": {
    "id": "myId",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:cmfsotelo/outsystems-templatePlugin.git"
  },
  "keywords": [
    "cordova",
    "outsystems"
  ],
  "standard-version": {
    "commitAll": true,
    "message": "chore(release): raise version to %s",
    "tag-prefix": "",
    "skip": {
      "changelog": true
    }
  },
  "author": "OutSystems",
  "homepage": "https://github.com:cmfsotelo/outsystems-templatePlugin#readme",
  "dependencies": {
    "@trapezedev/configure": "^7.0.8",
    "replace-in-file": "^6.3.5",
    "xmldom": "^0.6.0",
    "xpath": "^0.0.32"
  }
}
